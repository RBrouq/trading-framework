{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Debug Framework",
      "type": "debugpy",
      "request": "launch",
      "module": "trading_framework",
      "justMyCode": true,
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}